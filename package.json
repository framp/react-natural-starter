{
  "name": "react-natural-starter",
  "version": "0.0.1",
  "description": "A simple application",
  "main": "./index.js",
  "scripts": {
    "start": "node ./index.js",
    "build": "browserify --extension=.jsx ./client.jsx > public/js/app.js",
    "watch": "nodemon -e js,jsx --ignore public/ --ignore test/ --exec npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.12.3",
    "express": "^4.12.3",
    "node-require-directory": "^1.0.2",
    "react": "^0.13.3",
    "react-natural": "git://github.com/framp/react-natural.git",
    "react-router": "^0.13.3",
    "react-router-form": "git://github.com/framp/react-router-form.git",
    "require-dir": "^0.3.0",
    "whatwg-fetch": "^0.9.0"
  },
  "devDependencies": {
    "babel": "^5.2.17",
    "browserify": "^10.2.3",
    "browserify-shim": "^3.8.8",
    "es6ify": "^1.6.0",
    "nodemon": "^1.3.7",
    "reactify": "^1.1.1"
  },
  "browserify": {
    "transform": [
      "reactify",
      "es6ify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "react": "global:React",
    "react-router": "global:ReactRouter",
    "react-router-form": "global:ReactRouterForm"
  }
}
